{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:lod0": "node scripts/build-lod0-tiles.js --tile 512 --out public/tiles/lod0",
    "dev": "next dev -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next dev -p 9002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:deps": "knip && depcheck || true",
    "analyze:ts": "ts-prune || true",
    "analyze:graphs": "madge --circular src || true",
    "size": "size-limit",
    "size:why": "cross-env ANALYZE=true next build",
    "sitemap": "next-sitemap",
    "seo:check": "node -e \"console.log('Use next-seo via componentes/metadados')\"",
    "lhci:ci": "lhci autorun || true",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "public/sw.js",
      "limit": "150 kB"
    }
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
