{
  "ci": {
    "collect": {
      "startServerCommand": "npm run dev",
      "url": ["http://localhost:9002"],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.7 }],
        "categories:pwa": ["warn", { "minScore": 0.7 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
